
while wait() do
	local args = {
	    [1] = "FlowerGarden",
	    [2] = "InstaGrowSeed",
	    [3] = 1
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Instancing_InvokeCustomFromClient"):InvokeServer(unpack(args))

	local args = {
	    [1] = "FlowerGarden",
	    [2] = "InstaGrowSeed",
	    [3] = 2
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Instancing_InvokeCustomFromClient"):InvokeServer(unpack(args))

	local args = {
	    [1] = "FlowerGarden",
	    [2] = "InstaGrowSeed",
	    [3] = 3
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Instancing_InvokeCustomFromClient"):InvokeServer(unpack(args))

	local args = {
	    [1] = "FlowerGarden",
	    [2] = "InstaGrowSeed",
	    [3] = 4
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Instancing_InvokeCustomFromClient"):InvokeServer(unpack(args))

	local args = {
	    [1] = "FlowerGarden",
	    [2] = "InstaGrowSeed",
	    [3] = 5
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Instancing_InvokeCustomFromClient"):InvokeServer(unpack(args))
	
	local args = {
	    [1] = "FlowerGarden",
	    [2] = "InstaGrowSeed",
	    [3] = 6
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Instancing_InvokeCustomFromClient"):InvokeServer(unpack(args))
	
	local args = {
	    [1] = "FlowerGarden",
	    [2] = "InstaGrowSeed",
	    [3] = 7
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Instancing_InvokeCustomFromClient"):InvokeServer(unpack(args))
end

