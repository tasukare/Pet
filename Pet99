local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/tasukare/idk/main/Kavo"))()
local Window = Library.CreateLib("Ngoun sovannarong", "Ocean")

local Tab = Window:NewTab("Autofarm")
local Section = Tab:NewSection("autofarm")
Section:NewButton("Plant", "ButtonInfo", function()
    while wait() do
        local args = {
        [1] = "FlowerGarden",
        [2] = "PlantSeed",
        [3] = 1,
        [4] = "Diamond"
        }

        game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Instancing_InvokeCustomFromClient"):InvokeServer(unpack(args))
    end
end)

Section:NewButton("Plant", "ButtonInfo", function()
        while wait() do
            local args = {
            [1] = "FlowerGarden",
            [2] = "PlantSeed",
            [3] = 2,
            [4] = "Diamond"
            }

        game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Instancing_InvokeCustomFromClient"):InvokeServer(unpack(args))
        end
    end)

end)
